<mvc:View
    controllerName="sp.payrollEngine.controller.PayrollProcessing"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:tnt="sap.tnt">
    <tnt:ToolPage id="toolPage">
        <tnt:sideContent>
            <tnt:SideNavigation id="_IDGenSideNavigation1">
                <tnt:NavigationList id="_IDGenNavigationList1" itemSelect="onItemSelect">
                    <tnt:items>
                        <tnt:NavigationListItem id="_IDGenNavigationListItem1" text="Home" icon="sap-icon://home" key="home"/>
                        <tnt:NavigationListItem id="_IDGenNavigationListItem2" text="Payroll" icon="sap-icon://lead" key="payrollProcessing"/>
                        <tnt:NavigationListItem id="_IDGenNavigationListItem3" text="People" icon="sap-icon://group" key="people"/>
                        <tnt:NavigationListItem id="_IDGenNavigationListItem4" text="Third-party Integration" icon="sap-icon://home" key="thirdPartyIntegration"/>
                        <tnt:NavigationListItem id="_IDGenNavigationListItem5" text="Reports" icon="sap-icon://expense-report" key="reports"/>
                        <tnt:NavigationListItem id="_IDGenNavigationListItem6" text="General Settings" icon="sap-icon://settings">
                            <tnt:items>
                                <tnt:NavigationListItem id="_IDGenNavigationListItem7" text="Taxes" icon="sap-icon://money-bills" key="taxes"/>
                                <tnt:NavigationListItem id="_IDGenNavigationListItem8" text="Social insurance" icon="sap-icon://insurance-life" key="socialInsurance"/>
                                <tnt:NavigationListItem id="_IDGenNavigationListItem9" text="Gratuity" key="gratuity"/>
                                <tnt:NavigationListItem id="_IDGenNavigationListItem10" text="Absences" key="absences"/>
                                <tnt:NavigationListItem id="_IDGenNavigationListItem11" text="Wage Types" key="wageTypes"/>
                                <tnt:NavigationListItem id="_IDGenNavigationListItem12" text="Loans" icon="sap-icon://loan" key="loans"/>
                                <tnt:NavigationListItem id="_IDGenNavigationListItem13" text="Mapping" key="mapping"/>
                            </tnt:items>
                        </tnt:NavigationListItem>
                    </tnt:items>
                </tnt:NavigationList>
            </tnt:SideNavigation>
        </tnt:sideContent>
        <tnt:mainContents>
            <Page id="payrollProcessingPage">
                <headerContent>
                    <Bar id="_IDGenBar1" class="darkBlueHeader">
                        <contentLeft>
                            <Text id="_IDGenText1" text="Raptors" class="largeText sapUiSmallMargin whiteText"/>
                        </contentLeft>
                        <contentMiddle>
                            <SearchField id="_IDGenSearchField1" width="60%" placeholder="Search..."/>
                        </contentMiddle>
                        <contentRight>
                            <Button id="_IDGenButton11" icon="images/ticket.png" press="onTicketButtonClick"></Button>
                            <Button id="_IDGenButton12" icon="images/help.png" press="onOIPButtonClick"></Button>
                            <Button id="_IDGenButton13" icon="images/notification.png" press="onNotificationButtonClick"></Button>
                            <Button id="_IDGenButton14" icon="images/ProfileSetting.png" press="onProfileSettingButtonClick"></Button>
                        </contentRight>
                    </Bar>
                </headerContent>
                <content>
                    <VBox id="_IDGenVBox3" height="25px"></VBox>
                    <VBox id="_IDGenVBox1" class="sapUiSmallMargin">
                        <Title id="_IDGenTitle1" level="H1" text="1. Enter Payroll" />
                        <HBox id="_IDGenHBox1" class="sapUiTinyMarginBottom">
                            <MessageStrip id="_IDGenMessageStrip1"
                                text="You have outstanding new hires that aren't included in this payroll."
                                showCloseButton="false"
                                showIcon="true"
                                type="Warning">
                                <link>
                                    <Link id="_IDGenLink1" text="See outstanding new hires" />
                                </link>
                            </MessageStrip>
                        </HBox>
                        <VBox id="_IDGenVBox2" class="sapUiSmallMargin sapUiLargeMarginBottom">
                            <HBox id="_IDGenHBox5" class="sapUiTinyMarginBottom">
                                <GenericTile id="_IDGenGenericTile1" header="Biweekly" subheader="" width="370px">
                                    <TileContent id="_IDGenTileContent1">
                                        <VBox id="_IDGenVBox4">
                                            <HBox id="_IDGenHBox6">
                                                <Text id="_IDGenText33" text="Pay Period: Apr 01, 2022 â€“ Apr 15, 2022" />
                                            </HBox>
                                            <HBox id="_IDGenHBox7" alignItems="Center">
                                                <Text id="_IDGenText34" text="Check date: Apr 20, 2022" />
                                                <core:Icon id="_IDGenIcon1" src="sap-icon://warning" color="Critical" class="sapUiTinyMarginBegin" />
                                                <Button id="_IDGenButton10" icon="sap-icon://edit" type="Transparent" class="sapUiTinyMarginBegin" />
                                            </HBox>
                                        </VBox>
                                    </TileContent>
                                </GenericTile>
                            </HBox>
                            <HBox id="_IDGenHBox3" class="sapUiTinyMarginBottom">
                                <Text id="_IDGenText6" text="View" class="sapUiTinyMarginEnd"/>
                                <Select id="_IDGenSelect1" items="{/employees}" selectedKey="all" class="sapUiTinyMarginEnd">
                                    <core:Item id="_IDGenItem1" key="all" text="All Employees" />
                                    <core:Item id="_IDGenItem2" key="active" text="Active Employees" />
                                    <core:Item id="_IDGenItem3" key="inactive" text="Inactive Employees" />
                                </Select>
                                <Text id="_IDGenText7" text="Sort by" class="sapUiTinyMarginEnd" />
                                <Select id="_IDGenSelect2" items="{/sortOptions}" selectedKey="lastName">
                                    <core:Item id="_IDGenItem4" key="lastName" text="Last Name" />
                                    <core:Item id="_IDGenItem5" key="firstName" text="First Name" />
                                    <core:Item id="_IDGenItem6" key="department" text="Department" />
                                </Select>
                            </HBox>
                        </VBox>
                        
                        <Table id="employeeTable" inset="false" items="{/payrollItems}">
                            <columns>
                                <Column id="_IDGenColumn1">
                                    <Text id="_IDGenText8" text="Name" />
                                </Column>
                                <Column id="_IDGenColumn2">
                                    <Text id="_IDGenText9" text="Department" />
                                </Column>
                                <Column id="_IDGenColumn3">
                                    <Text id="_IDGenText10" text="Rate" />
                                </Column>
                                <Column id="_IDGenColumn4">
                                    <Text id="_IDGenText11" text="Regular Hours" />
                                </Column>
                                <Column id="_IDGenColumn5">
                                    <Text id="_IDGenText12" text="Salary Amount" />
                                </Column>
                                <Column id="_IDGenColumn6">
                                    <Text id="_IDGenText13" text="Overtime Hours" />
                                </Column>
                                <Column id="_IDGenColumn7">
                                    <Text id="_IDGenText14" text="Vacation Hours" />
                                </Column>
                                <Column id="_IDGenColumn8">
                                    <Text id="_IDGenText15" text="Sick Hours" />
                                </Column>
                                <Column id="_IDGenColumn9">
                                    <Text id="_IDGenText16" text="Personal Hours" />
                                </Column>
                                <Column id="_IDGenColumn10">
                                    <Text id="_IDGenText17" text="Holiday Hours" />
                                </Column>
                                <Column id="_IDGenColumn11">
                                    <Text id="_IDGenText18" text="Bonus Amount" />
                                </Column>
                                <Column id="_IDGenColumn12">
                                    <Text id="_IDGenText19" text="Misc Amount" />
                                </Column>
                                <Column id="_IDGenColumn13">
                                    <Text id="_IDGenText20" text="Misc Reimb" />
                                </Column>
                            </columns>
                            <items>
                                <ColumnListItem id="_IDGenColumnListItem1">
                                    <cells>
                                        <ObjectIdentifier id="_IDGenObjectIdentifier1" title="{name}" />
                                        <Text id="_IDGenText21" text="{department}" />
                                        <Text id="_IDGenText22" text="{rate}" />
                                        <Text id="_IDGenText23" text="{regularHours}" />
                                        <Text id="_IDGenText24" text="{salaryAmount}" />
                                        <Text id="_IDGenText25" text="{overtimeHours}" />
                                        <Text id="_IDGenText26" text="{vacationHours}" />
                                        <Text id="_IDGenText27" text="{sickHours}" />
                                        <Text id="_IDGenText28" text="{personalHours}" />
                                        <Text id="_IDGenText29" text="{holidayHours}" />
                                        <Text id="_IDGenText30" text="{bonusAmount}" />
                                        <Text id="_IDGenText31" text="{miscAmount}" />
                                        <Text id="_IDGenText32" text="{miscReimb}" />
                                    </cells>
                                </ColumnListItem>
                            </items>
                        </Table>
                        <HBox id="_IDGenHBox4" class="sapUiSmallMarginTop" justifyContent="SpaceBetween">
                            <HBox id="_IDGenHBoxLeftButtons">
                                <Button id="_IDGenButton2" text="Payroll Overrides" />
                                <Button id="_IDGenButton3" text="Sort By" />
                                <Button id="_IDGenButton4" text="Add Employee" />
                            </HBox>
                            <HBox id="_IDGenHBoxRightButtons">
                                <Button id="_IDGenButton5" text="Cancel" />
                                <Button id="_IDGenButton6" text="Previous" />
                                <Button id="_IDGenButton7" text="Save" />
                                <Button id="_IDGenButton8" text="Finish Later" />
                                <Button id="_IDGenButton9" text="Next" type="Emphasized" press="onNext" />
                            </HBox>
                        </HBox>
                    </VBox>
                </content>
            </Page>
        </tnt:mainContents>
    </tnt:ToolPage>
</mvc:View>
