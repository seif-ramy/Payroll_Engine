<mvc:View
    controllerName="sp.payrollEngine.controller.PayrollPreview"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:tnt="sap.tnt">

      
    <tnt:ToolPage id="toolPage">
        <tnt:sideContent>
            <tnt:SideNavigation id="_IDGenSideNavigation1">
                <tnt:NavigationList id="_IDGenNavigationList1" itemSelect="onItemSelect">
                    <tnt:items>
                    <tnt:NavigationListItem id="_IDGenNavigationListItem1" text="Home" icon="sap-icon://home" ></tnt:NavigationListItem>
                    <tnt:NavigationListItem id="_IDGenNavigationListItem2" text="Payroll" icon="sap-icon://lead" ></tnt:NavigationListItem>
                    <tnt:NavigationListItem id="_IDGenNavigationListItem3" text="People" icon="sap-icon://group" ></tnt:NavigationListItem>
                    <tnt:NavigationListItem id="_IDGenNavigationListItem4" text="Third-party Integration" icon="sap-icon://home" ></tnt:NavigationListItem>
                    <tnt:NavigationListItem id="_IDGenNavigationListItem5" text="Reports" icon="sap-icon://expense-report" ></tnt:NavigationListItem>
                    <tnt:NavigationListItem id="_IDGenNavigationListItem6" text="General Settings" icon="sap-icon://settings">
                    <tnt:NavigationListItem id="_IDGenNavigationListItem7"  text="Taxes" icon="sap-icon://money-bills"></tnt:NavigationListItem>
                    <tnt:NavigationListItem id="_IDGenNavigationListItem8"  text="Social insurance" icon="sap-icon://insurance-life"></tnt:NavigationListItem>
                    <tnt:NavigationListItem id="_IDGenNavigationListItem9"  text="Gratuity"></tnt:NavigationListItem>
                    <tnt:NavigationListItem id="_IDGenNavigationListItem10"  text="Absences"></tnt:NavigationListItem>
                    <tnt:NavigationListItem id="_IDGenNavigationListItem11"  text="Wage Types"></tnt:NavigationListItem>
                    <tnt:NavigationListItem id="_IDGenNavigationListItem12"  text="Loans" icon="sap-icon://loan"></tnt:NavigationListItem>
                    <tnt:NavigationListItem id="_IDGenNavigationListItem13"  text="Mapping"  ></tnt:NavigationListItem>
                    </tnt:NavigationListItem>
                    </tnt:items>
                </tnt:NavigationList>
            </tnt:SideNavigation>
        </tnt:sideContent>
        <tnt:mainContents>
            <Page id="payrollPreviewPage" >
              <headerContent>
            <!-- Header content goes here -->
            <Bar class="darkBlueHeader">
                <contentLeft>
                    <!-- Content for the left side of the header -->
                    <Text id="_IDGenText1" text="Raptors" class="largeText sapUiSmallMargin whiteText"/>
                </contentLeft>
                <contentMiddle>
                    <!-- Content for the middle of the header -->
                    <SearchField id="_IDGenSearchField1" width="60%" placeholder="Search..."/>
                </contentMiddle>
                <contentRight>
                    <Button id="_IDGenButton11" icon="images/ticket.png" press="onTicketButtonClick"></Button>
                    <Button id="_IDGenButton12" icon="images/help.png"  press="onOIPButtonClick"></Button>
                    <Button id="_IDGenButton13" icon="images/notification.png" press="onNotificationButtonClick"></Button>
                    <Button id="_IDGenButton14" icon="images/ProfileSetting.png" press="onProfileSettingButtonClick"></Button>
                </contentRight>
            </Bar>
        </headerContent>

                <content>
             <VBox id="_IDGenVBox2" height="25px"></VBox>
    <HBox class="container">
                <!-- Circle 1 -->
                <core:Icon src="images/filledCircle.png" size="2rem" class="circle leftMarginForLine"/>
                
                <!-- Straight Line -->
                <core:Icon src="sap-icon://line" size="2rem" class="straightLine"/>

                <!-- Circle 2 -->
                <core:Icon src="images/filledCircle.png" size="2rem" class="circle"/>
                
                <!-- Straight Line -->
                <core:Icon src="sap-icon://line" size="2rem" class="straightLine"/>

                <!-- Circle 3 -->
                <core:Icon src="images/filledCircle.png" size="2rem" class="circleNotFilled"/>
            </HBox>

            <VBox>
                 <VBox id="_IDGenVBox3" height="50px"></VBox>
                 <Text text="Preview Payroll" class="largeText SmallLeftMargin"/>
                 <VBox id="_IDGenVBox4" height="25px"></VBox>
            </VBox>
            <HBox class="container">
    <!-- First box for Cash Required -->
                <VBox class="boxContainer sapUiSmallMargin">
                    <Text text="Cash Required" class="headerText"/>
                    <Text text="$3344" class="contentText"/>
                    <Text text="Biweekly" class="contentText"/>
                    <Text text="Any Date to Any Date" class="contentText"/>
                </VBox>
                <VBox class="boxContainer sapUiSmallMargin">
                    <Text text="Payroll Dates" class="headerText"/>
                    <Text text="" class="contentText"/>
                    <Text text="" class="contentText"/>
                    <Text text="Check Date" class="contentText"/>
                    <Text text="Wed,April 20,2022" class="contentText"/>
                </VBox>
                <VBox class="boxContainer sapUiSmallMargin">
                    <Text text="People" class="headerText SmallLeftMargin"/>
                    <HBox justifyContent="SpaceBetween" alignItems="Center">  
                        <Text text="3" class="blueText largerText SmallLeftMargin" />
                        <HBox id="_IDGenHBox7" justifyContent="End">
                            <Text text="0" class="blueText largerText RightMarginForLine" />
                        </HBox>
                    </HBox>
                    <HBox justifyContent="SpaceBetween" alignItems="Center">  
                        <Text text="Employees" class="contentText SmallLeftMargin greyText"/>
                        <HBox justifyContent="End">
                            <Text text="Contractors" class="contentText SmallLeftMargin greyText"/>
                        </HBox>
                    </HBox>
                    <Text text="Check Date" class="contentText SmallLeftMargin"/>
                    <Text text="Wed,April 20,2022" class="contentText SmallLeftMargin"/>
                </VBox>
        </HBox>
        <Table id="employeeTable" growing="true" growingScrollToLoad="true">
            <columns>
                <Column>
                    <Text text="Employee Name" class="greyText"/>
                </Column>
                <Column>
                    <Text text="Type" class="greyText"/>
                </Column>
                <Column>
                    <Text text="Total Hours" class="greyText"/>
                </Column>
                <Column>
                    <Text text="Gross Pay" class="greyText"/>
                </Column>
                <Column>
                    <Text text="Taxes" class="greyText"/>
                </Column>
                <Column>
                    <Text text="Deductions" class="greyText"/>
                </Column>
                <Column>
                    <Text text="Net Pay" class="greyText"/>
                </Column>
                <Column>
                    <Text text="Employee Taxes" class="greyText"/>
                </Column>
            </columns>
    <items>
        <ColumnListItem>
            <cells>
                <Text text="2-Dept" />  
            </cells>
        </ColumnListItem>

        <ColumnListItem>   
            <cells>
                <Text text="Don, May" class="blueeText" />
                <Text text="Regular"/>
                <Text text="0.00"/>
                <Text text="$1600.00"/>
                <Text text="$271.68"/>
                <Text text="$0.00"/>
                <Text text="$1,228.32"/>
                <Text text="$156.75"/>
            </cells>
        </ColumnListItem>
        <ColumnListItem>
            <cells>
              <Text text="Doe, Jane" class ="blueeText"/>
                <Text text="Regular"/>
                <Text text="25.00"/>
                <Text text="$500.00"/>
                <Text text="$85.04"/>
                <Text text="$35.00"/>
                <Text text="$379.96.32"/>
                <Text text="$52.25"/>
            </cells>
        </ColumnListItem>
        <ColumnListItem>
            <cells>
                <Text text="Smith, John" class ="blueeText"/>
                <Text text="Regular"/>
                <Text text="70.00"/>
                <Text text="$1050.00"/>
                <Text text="$207.00"/>
                <Text text="$0.00"/>
                <Text text="$843.00"/>
                <Text text="$109.73"/>
            </cells>
        </ColumnListItem>
        <ColumnListItem>
            <cells>
                <Text text="Totals"/>
                <Text text=""/>
                <Text text="95.00"/>
                <Text text="$3,050.00"/>
                <Text text="$563.72"/>
                <Text text="$35"/>
                <Text text="$2451.28"/>
                <Text text="$318.73"/>
            </cells>    
        </ColumnListItem>
    </items>
    </Table>
                </content>
            </Page>
        </tnt:mainContents>
    </tnt:ToolPage>
</mvc:View>