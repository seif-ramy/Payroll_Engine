<mvc:View 
    controllerName="sp.payrollEngine.controller.PayrollPreview" 
    xmlns:mvc="sap.ui.core.mvc" 
    displayBlock="true"
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:tnt="sap.tnt">

    <tnt:ToolPage id="toolPage">
        <tnt:sideContent>
            <tnt:SideNavigation id="_IDGenSideNavigation1">
                <tnt:NavigationList id="_IDGenNavigationList1" itemSelect="onItemSelect">
                    <tnt:items>
					<tnt:NavigationListItem id="_IDGenNavigationListItem1" text="Home" icon="sap-icon://home" ></tnt:NavigationListItem>
					<tnt:NavigationListItem id="_IDGenNavigationListItem2" text="Payroll" icon="sap-icon://lead" ></tnt:NavigationListItem>
					<tnt:NavigationListItem id="_IDGenNavigationListItem3" text="People" icon="sap-icon://group" ></tnt:NavigationListItem>
					<tnt:NavigationListItem id="_IDGenNavigationListItem4" text="Third-party Integration" icon="sap-icon://home" ></tnt:NavigationListItem>
					<tnt:NavigationListItem id="_IDGenNavigationListItem5" text="Reports" icon="sap-icon://expense-report" ></tnt:NavigationListItem>
					<tnt:NavigationListItem id="_IDGenNavigationListItem6" text="General Settings" icon="sap-icon://settings">
					<tnt:NavigationListItem id="_IDGenNavigationListItem7"  text="Taxes" icon="sap-icon://money-bills"></tnt:NavigationListItem>
					<tnt:NavigationListItem id="_IDGenNavigationListItem8"  text="Social insurance" icon="sap-icon://insurance-life"></tnt:NavigationListItem>
					<tnt:NavigationListItem id="_IDGenNavigationListItem9"  text="Gratuity"></tnt:NavigationListItem>
					<tnt:NavigationListItem id="_IDGenNavigationListItem10"  text="Absences"></tnt:NavigationListItem>
					<tnt:NavigationListItem id="_IDGenNavigationListItem11"  text="Wage Types"></tnt:NavigationListItem>
					<tnt:NavigationListItem id="_IDGenNavigationListItem12"  text="Loans" icon="sap-icon://loan"></tnt:NavigationListItem>
					<tnt:NavigationListItem id="_IDGenNavigationListItem13"  text="Mapping"  ></tnt:NavigationListItem>
					</tnt:NavigationListItem>
					</tnt:items>
                </tnt:NavigationList>
            </tnt:SideNavigation>
        </tnt:sideContent>
        
        <tnt:mainContents>
            <Page id="payrollPreviewPage" title="Preview Payroll">
                <content>
                    <Button id="btnApprovePayrollPreview" text="Approve" press="onApprove" />
                </content>
            </Page>
        </tnt:mainContents>
    </tnt:ToolPage>
    
</mvc:View>
